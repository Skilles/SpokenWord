{
  "schema_version": 1,
  "mixin": [
    "spokenword.mixins.json",
    "spokenword-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "spokenword",
    "version": "${version}",
    "metadata": {
      "name": "Spoken Word",
      "description": "This is an example description! Tell everyone what your mod is about!",
      "contributors": {
        "Me!": "Owner"
      },
      "contact": {
        "sources": "https://github.com/architectury/architectury-templates"
      },
      "icon": "assets/spokenword/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.spokenword.quilt.SpokenWordQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.1.12"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}